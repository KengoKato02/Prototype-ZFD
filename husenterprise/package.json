{
  "name": "@husenterprise/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "lint:filesystem": "ls-lint",
    "lint:pkg": "npmPkgJsonLint .",
    "postbuild": "./scripts/postbuild.sh",
    "lint:workspace": "nx workspace-lint",
    "lint:unimported": "echo 'unimported'",
    "vercel-build": "echo 'No Build Needed'",
    "format:write": "nx format:write --all",
    "test": "nx run-many --target=test --all",
    "start": "nx run-many --target=start --all",
    "serve": "nx run-many --target=serve --all",
    "build": "nx run-many --target=build --all",
    "deploy": "nx run-many --target=deploy --all",
    "format": "nx run-many --target=format --all",
    "typecheck": "nx run-many --target=typecheck --all",
    "lint": "eslint . --cache && nx run-many --target=lint --all",
    "deploy:local": "pnpm build && pnpm postbuild && vercel deploy --prod",
    "lint:fix": "eslint . --fix --cache && nx run-many --target=lint:fix --all",
    "postinstall": "find apps -maxdepth 2 -name package.json -execdir pnpm install \\; || true",
    "lint:root": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(root)\""
  },
  "private": true,
  "devDependencies": {
    "@nx/esbuild": "17.2.8",
    "@nx/eslint": "17.2.8",
    "@nx/eslint-plugin": "17.2.8",
    "@nx/js": "17.2.8",
    "@nx/workspace": "17.2.8",
    "@swc-node/register": "~1.6.7",
    "@swc/core": "~1.3.85",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "concurrently": "^8.2.2",
    "esbuild": "^0.19.2",
    "eslint": "~8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "nx": "17.2.8",
    "prettier": "^2.6.2",
    "typescript": "~5.2.2"
  }
}
